class Solution {
    public int[] maxSlidingWindow(int[] nums, int k) {
        if (k == 0 || k == 1) {
            return nums;
        }
        int [] maxArr = new int [nums.length - k  + 1];
        for (int i = 0 ; i < maxArr.length; ++i) {
            int max = nums[i];
            for (int j = i; j < nums.length && j < i + k; ++j) {
                max = Math.max(nums[j], max);
            }
            maxArr[i] = max;
        }
        return maxArr;
    }
}
